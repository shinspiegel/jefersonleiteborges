  "scripts": {
    "dev": "rm -rf ./dist; parcel -p 5050 watch src/index.html --https --cert .certs/cert.pem --key .certs/key.pem",
    "dev:http": "parcel -p 5050 watch src/index.html",
    "build": "rm -rf build; parcel build src/index.html --out-dir build --no-source-maps",
    "test": "jest",
    "deploy:master": "yarn build; aws s3 rm s3://cdn.jeferson.me --recursive; aws s3 sync build/ s3://cdn.jeferson.me --acl public-read",
    "deploy:dev": "yarn build; aws s3 rm s3://cdn.dev.jeferson.me --recursive; aws s3 sync build/ s3://cdn.dev.jeferson.me --acl public-read",
    "clean": "rm -rf .cache .certs build dist docs node_modules"
  },
